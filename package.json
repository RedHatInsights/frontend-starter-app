    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "eslint": "8.18.0",
    "identity-obj-proxy": "3.0.0",
