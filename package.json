    "@redhat-cloud-services/frontend-components-utilities": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.0",
    "react-router-dom": "^6.12.0",
    "redux": "4.2.1",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "6.1.2"
